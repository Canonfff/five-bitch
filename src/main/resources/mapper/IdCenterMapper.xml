<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.canon.service.dao.IdCenterMapper">
	<resultMap id="BaseResultMap" type="com.canon.model.platform.IdCenter">
		<result column="id" property="id" jdbcType="BIGINT"/>
		<result column="app" property="app" jdbcType="VARCHAR" />
		<result column="tableName" property="tableName" jdbcType="VARCHAR" />
		<result column="cindex" property="cindex" jdbcType="BIGINT" />
		<result column="createtime" property="createtime" jdbcType="TIMESTAMP" />
		<result column="updatetime" property="updatetime" jdbcType="TIMESTAMP" />
		<result column="keypre" property="keypre" jdbcType="VARCHAR" />
		<result column="fstep" property="fstep" jdbcType="INTEGER" />

	</resultMap>

	<sql id="Base_Column_List">
		id,app,tableName,cindex,createtime,keypre,fstep
	</sql>

	<select id="findAll" resultType="com.canon.model.platform.IdCenter" parameterType="java.lang.Long">
		select
		<include refid="Base_Column_List" />
		from t_idcenter
		where id = #{id}
	</select>

    <select id="selectByPrimaryKey" resultType="com.canon.model.platform.IdCenter"
            parameterType="java.lang.Long">
        select
        <include refid="Base_Column_List"/>
        from t_idcenter
        where id = #{id,jdbcType=BIGINT}
    </select>
</mapper>
